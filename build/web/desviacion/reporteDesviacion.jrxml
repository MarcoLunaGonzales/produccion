     inner join CAPAS_ASEGURAMIENTO_CALIDAD cac on cac.COD_CAPA_ASEGURAMIENTO_CALIDAD =cacd.COD_CAPA_ASEGURAMIENTO_CALIDAD
     inner join TIPOS_CAPAS_ASEGURAMIENTO_CALIDAD tca on tca.COD_TIPO_CAPA_ASEGURAMIENTO_CALIDAD =cac.COD_TIPO_CAPA_ASEGURAMIENTO_CALIDAD
