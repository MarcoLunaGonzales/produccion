	<subDataset name="dataSetEspecificacionesFisicas" uuid="770649b2-191c-40f1-9a96-d65ac1c73785">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Oficial"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="codVersion" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select efc.NOMBRE_ESPECIFICACION,
(case 
                  when efc.COD_TIPO_RESULTADO_ANALISIS  =1 then efp.DESCRIPCION
                  when efc.COD_TIPO_RESULTADO_ANALISIS =2 then cast(efp.LIMITE_INFERIOR as varchar)+' '+efc.UNIDAD+' - '+cast(efp.LIMITE_SUPERIOR as varchar)+' '+efc.UNIDAD
                  when efc.COD_TIPO_RESULTADO_ANALISIS =4 then cast(efp.VALOR_EXACTO  as varchar)
                  else cast(efp.VALOR_EXACTO as varchar)
           end) as especificacionReporte
from ESPECIFICACIONES_FISICAS_PRODUCTO efp 
	inner join ESPECIFICACIONES_FISICAS_CC efc on efc.COD_ESPECIFICACION=efp.COD_ESPECIFICACION
    
where efp.COD_VERSION= $P{codVersion} 
	and 
    (
    	efc.NOMBRE_ESPECIFICACION like '%aspecto%'
        or efc.NOMBRE_ESPECIFICACION like '%color%'
        or efc.NOMBRE_ESPECIFICACION like '%peso%tubo%'
        or efc.NOMBRE_ESPECIFICACION like '%aspecto%tubo%'
        
	)
order by efc.NOMBRE_ESPECIFICACION]]>
		</queryString>
		<field name="NOMBRE_ESPECIFICACION" class="java.lang.String"/>
		<field name="especificacionReporte" class="java.lang.String"/>
	</subDataset>
