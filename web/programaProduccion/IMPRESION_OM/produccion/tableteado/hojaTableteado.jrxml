	<subDataset name="dataSetEspecificacionesFisicas" uuid="770649b2-191c-40f1-9a96-d65ac1c73785">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Oficial"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="codVersion" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select efc.NOMBRE_ESPECIFICACION,
(case 
                  when efc.COD_TIPO_RESULTADO_ANALISIS  =1 then efp.DESCRIPCION
                  when efc.COD_TIPO_RESULTADO_ANALISIS =2 then cast(efp.LIMITE_INFERIOR as varchar)+' '+efc.UNIDAD+' - '+cast(efp.LIMITE_SUPERIOR as varchar)+' '+efc.UNIDAD
                  when efc.COD_TIPO_RESULTADO_ANALISIS =4 then cast(efp.VALOR_EXACTO  as varchar)
                  else cast(efp.VALOR_EXACTO as varchar)
           end) as especificacionReporte
from ESPECIFICACIONES_FISICAS_PRODUCTO efp 
	inner join ESPECIFICACIONES_FISICAS_CC efc on efc.COD_ESPECIFICACION=efp.COD_ESPECIFICACION
    
where efp.COD_VERSION= $P{codVersion} 
	and 
    (
    	efc.NOMBRE_ESPECIFICACION like '%form%'
        or efc.NOMBRE_ESPECIFICACION like '%color%nucleo%'
        or efc.NOMBRE_ESPECIFICACION like '%dureza%'
        or efc.NOMBRE_ESPECIFICACION like '%ancho%promedi%'
        or efc.NOMBRE_ESPECIFICACION like '%espesor%'
        or efc.NOMBRE_ESPECIFICACION like '%largo%'
        or efc.NOMBRE_ESPECIFICACION like '%largo%'
	)
order by efc.NOMBRE_ESPECIFICACION]]>
		</queryString>
		<field name="NOMBRE_ESPECIFICACION" class="java.lang.String"/>
		<field name="especificacionReporte" class="java.lang.String"/>
	</subDataset>
	<parameter name="codProgramaProd" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[443]]></defaultValueExpression>
	</parameter>
	<parameter name="codLoteProduccion" class="java.lang.String" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA["205827"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select top 1 ip.INDICACION_PROCESO as precauciones,
    ip1.INDICACION_PROCESO as precaucionesPersonal,
	ip2.INDICACION_PROCESO as indicacionesRendimiento,
    ip3.INDICACION_PROCESO as indicacionesEnvioCC,
       pp.COD_COMPPROD_VERSION
       ,cpv.PESO_TEORICO,um.ABREVIATURA as unidadPeso,cpv.PORCIENTO_LIMITE_ALERTA,cpv.PORCIENTO_LIMITE_ACCION
from PROGRAMA_PRODUCCION pp
	inner join COMPONENTES_PROD_VERSION cpv on cpv.COD_VERSION=pp.COD_COMPPROD_VERSION
	left outer join unidades_medida um on um.COD_UNIDAD_MEDIDA = cpv.COD_UNIDAD_MEDIDA_PESO_TEORICO
     left outer join INDICACION_PROCESO ip on ip.COD_VERSION =
     pp.COD_COMPPROD_VERSION and ip.COD_TIPO_INDICACION_PROCESO = 1 and
     ip.COD_PROCESO_ORDEN_MANUFACTURA = 7
     left outer join INDICACION_PROCESO ip1 on ip1.COD_VERSION =
     pp.COD_COMPPROD_VERSION and ip1.COD_TIPO_INDICACION_PROCESO = 22 and
     ip1.COD_PROCESO_ORDEN_MANUFACTURA = 7
     left outer join INDICACION_PROCESO ip2 on ip2.COD_VERSION =
     pp.COD_COMPPROD_VERSION and ip2.COD_TIPO_INDICACION_PROCESO = 29 and
     ip2.COD_PROCESO_ORDEN_MANUFACTURA = 7
     left outer join INDICACION_PROCESO ip3 on ip3.COD_VERSION =
     pp.COD_COMPPROD_VERSION and ip3.COD_TIPO_INDICACION_PROCESO = 26 and
     ip3.COD_PROCESO_ORDEN_MANUFACTURA = 7
where pp.COD_LOTE_PRODUCCION = $P{codLoteProduccion} and
      pp.COD_PROGRAMA_PROD = $P{codProgramaProd}
order by pp.COD_TIPO_PROGRAMA_PROD]]>
	</queryString>
	<field name="precauciones" class="java.lang.String"/>
	<field name="precaucionesPersonal" class="java.lang.String"/>
	<field name="indicacionesRendimiento" class="java.lang.String"/>
