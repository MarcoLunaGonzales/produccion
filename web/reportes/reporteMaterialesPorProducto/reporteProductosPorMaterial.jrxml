		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="local20160105"/>
		<parameter name="codSolicitudMantenimiento" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select m.COD_MATERIAL,
       m.NOMBRE_MATERIAL,
       um.NOMBRE_UNIDAD_MEDIDA,
       cp.nombre_prod_semiterminado,
       fm.CANTIDAD_LOTE,
       datosFm.CANTIDAD
from COMPONENTES_PROD cp
     inner join FORMULA_MAESTRA fm on fm.COD_COMPPROD = cp.COD_COMPPROD
     inner join 
     (
       select fmd.CANTIDAD,
              fmd.COD_FORMULA_MAESTRA,
              fmd.COD_MATERIAL
       from FORMULA_MAESTRA_DETALLE_MP fmd
       union
       select fmdr.CANTIDAD,
              fmdr.COD_FORMULA_MAESTRA,
              fmdr.COD_MATERIAL
       from FORMULA_MAESTRA_DETALLE_MR fmdr
     ) as datosFm on datosFm.COD_FORMULA_MAESTRA = fm.COD_FORMULA_MAESTRA
     INNER JOIN MATERIALES m on m.COD_MATERIAL = datosFm.COD_MATERIAL
     INNER JOIN UNIDADES_MEDIDA um on um.COD_UNIDAD_MEDIDA = m.COD_UNIDAD_MEDIDA
where cp.COD_ESTADO_COMPPROD = 1 and
      fm.COD_ESTADO_REGISTRO = 1 AND
      cp.COD_AREA_EMPRESA IN ( select a.codAreaEmpresa from dbo.##codAreaEmpresaMaterialesProductoTemp a )  AND
      cp.COD_TIPO_PRODUCCION in ( select t.codTipoProgramaProd from dbo.##codTipoProduccionMaterialesProductoTemp t)
order by m.NOMBRE_MATERIAL,
         cp.nombre_prod_semiterminado]]>
		</queryString>
