       fmd.CANTIDAD
from COMPONENTES_PROD cp
     inner join FORMULA_MAESTRA fm on fm.COD_COMPPROD = cp.COD_COMPPROD
     inner join COMPONENTES_PRESPROD cpp on cpp.COD_COMPPROD = cp.COD_COMPPROD
     and cpp.COD_ESTADO_REGISTRO = 1
     inner join FORMULA_MAESTRA_DETALLE_ES fmd on fmd.COD_FORMULA_MAESTRA =
     fm.COD_FORMULA_MAESTRA and fmd.COD_PRESENTACION_PRODUCTO =
     cpp.COD_PRESENTACION and fmd.COD_TIPO_PROGRAMA_PROD =
     cpp.COD_TIPO_PROGRAMA_PROD
     inner join PRESENTACIONES_PRODUCTO pp on pp.cod_presentacion =cpp.COD_PRESENTACION
     inner join TIPOS_PROGRAMA_PRODUCCION tpp on tpp.COD_TIPO_PROGRAMA_PROD =
     cpp.COD_TIPO_PROGRAMA_PROD
     INNER JOIN MATERIALES m on m.COD_MATERIAL = fmd.COD_MATERIAL
     INNER JOIN UNIDADES_MEDIDA um on um.COD_UNIDAD_MEDIDA = m.COD_UNIDAD_MEDIDA
where cp.COD_ESTADO_COMPPROD = 1 and
      fm.COD_ESTADO_REGISTRO = 1 and
      pp.cod_estado_registro = 1 AND
      cp.COD_AREA_EMPRESA IN ( select a.codAreaEmpresa from dbo.##codAreaEmpresaMaterialesProductoTemp a )  AND
      cp.COD_TIPO_PRODUCCION in ( select t.codTipoProgramaProd from dbo.##codTipoProduccionMaterialesProductoTemp t)
order by m.NOMBRE_MATERIAL,
         cp.nombre_prod_semiterminado,
         tpp.NOMBRE_TIPO_PROGRAMA_PROD]]>
